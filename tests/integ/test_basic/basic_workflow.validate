<?xml version="1.0"?>
<!DOCTYPE workflow []>
<workflow realtime="T" scheduler="lsf">
    <cycledef group="hourly">0 * * * * *</cycledef>
    <log>
        <cyclestr>log_task1.@Y@m@d@H</cyclestr>
    </log>
    <task cycledefs="hourly" maxtries="1" name="task1">
        <command>
            <cyclestr>runcommand @Y@m@d@H</cyclestr>
        </command>
        <jobname>
            <cyclestr>task1_@Y@m@d@H</cyclestr>
        </jobname>
        <join>
            <cyclestr>task1_@Y@m@d@H.join</cyclestr>
        </join>
        <account>test_account</account>
        <walltime>00:20:00</walltime>
        <queue>dev2_shared</queue>
        <memory>2056M</memory>
        <native>-R affinity[core]</native>
        <cores>1</cores>
    </task>
</workflow>
